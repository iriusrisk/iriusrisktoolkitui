                <pattern name="Condition with answer" pattern='${type}(${field} == "${value}", answer == true);'/>
            </condition>
            <action project="" value="ConclusionType.HIDDEN_::_pii.handled_::_Personal Identifiable Information is handled" name="Insert Conclusion" type="drools-without-variable-type">
                <pattern name="Action of insert conclusion" pattern='insertLogical(new Conclusion(${value-1}, "${value-2}", "${value-3}"));'/>
            </action>
            <content></content>
        </rule>
